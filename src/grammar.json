{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "mutt_compose",
  "rules": {
    "source_file": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_content"
        },
        {
          "type": "BLANK"
        }
      ]
    },
    "_content": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "header"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "\n"
              },
              {
                "type": "SYMBOL",
                "name": "header"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\n"
        },
        {
          "type": "STRING",
          "value": "\n"
        },
        {
          "type": "SYMBOL",
          "name": "body"
        }
      ]
    },
    "header": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "SYMBOL",
            "name": "header_key"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "header_value"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        }
      ]
    },
    "header_key": {
      "type": "PATTERN",
      "value": "[A-Za-z][-A-Za-z0-9]*"
    },
    "header_value": {
      "type": "PATTERN",
      "value": "[^\\n]+"
    },
    "body": {
      "type": "PATTERN",
      "value": "[\\s\\S]+"
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}