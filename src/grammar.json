{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "mutt_compose",
  "rules": {
    "source_file": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": []
        },
        {
          "type": "SYMBOL",
          "name": "_content"
        }
      ]
    },
    "_content": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_header_line"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": "\n"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "_header_line"
                  }
                ]
              }
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": "\n"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_header_line"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": "\n"
                  },
                  {
                    "type": "SYMBOL",
                    "name": "_header_line"
                  }
                ]
              }
            },
            {
              "type": "STRING",
              "value": "\n"
            },
            {
              "type": "STRING",
              "value": "\n"
            },
            {
              "type": "SYMBOL",
              "name": "body"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\n"
            },
            {
              "type": "SYMBOL",
              "name": "body"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "_header_line": {
      "type": "SYMBOL",
      "name": "header"
    },
    "header": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "PATTERN",
            "value": "[A-Za-z][-A-Za-z0-9]*"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[^\\n]*"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        }
      ]
    },
    "body": {
      "type": "PATTERN",
      "value": "[\\s\\S]+"
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}