{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "mutt_compose",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "headers"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "body"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "headers": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "header"
      }
    },
    "header": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "PATTERN",
            "value": "[A-Za-z][-A-Za-z0-9]*"
          }
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "value",
              "content": {
                "type": "PATTERN",
                "value": "[^\\n]*"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "body": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\n"
        },
        {
          "type": "PATTERN",
          "value": "[\\s\\S]*"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}