{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "mutt_compose",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "header"
          },
          {
            "type": "SYMBOL",
            "name": "quoted_line"
          },
          {
            "type": "SYMBOL",
            "name": "quote_attribution"
          },
          {
            "type": "SYMBOL",
            "name": "markdown_heading"
          },
          {
            "type": "SYMBOL",
            "name": "markdown_list_item"
          },
          {
            "type": "SYMBOL",
            "name": "markdown_code_fence"
          },
          {
            "type": "SYMBOL",
            "name": "text_line"
          },
          {
            "type": "SYMBOL",
            "name": "blank_line"
          }
        ]
      }
    },
    "header": {
      "type": "PATTERN",
      "value": "[A-Z][-A-Za-z0-9]*: [^\\n]*\\n"
    },
    "quoted_line": {
      "type": "PATTERN",
      "value": ">+ ?[^\\n]*\\n"
    },
    "quote_attribution": {
      "type": "PATTERN",
      "value": "On .+ wrote:\\n"
    },
    "markdown_heading": {
      "type": "PATTERN",
      "value": "#{1,6} +[^\\n]+\\n"
    },
    "markdown_list_item": {
      "type": "PATTERN",
      "value": "[-*+] +[^\\n]+\\n"
    },
    "markdown_code_fence": {
      "type": "PATTERN",
      "value": "```[^\\n]*\\n"
    },
    "blank_line": {
      "type": "PATTERN",
      "value": "\\n"
    },
    "text_line": {
      "type": "PATTERN",
      "value": "[^#>\\-*+\\n][^\\n]*\\n"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}